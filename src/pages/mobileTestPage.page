<apex:page standardController="Account" docType="html-5.0" sidebar="false" applyBodyTag="false">
    <!--<apex:stylesheet value="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css"/>-->
    <apex:stylesheet value="//cdn.jsdelivr.net/webjars/bootstrap-sf1/0.1.0-beta.6/css/bootstrap-namespaced.css"/>
    <apex:includeScript value="//ajax.googleapis.com/ajax/libs/angularjs/1.3.7/angular.min.js"/>

<body class="bootstrap-sf1">    

    <div class="page-header page-header-anchor page-header-center context-account">
        <div class="container">
            <h3>{!Account.Name}</h3>
        </div>
    </div>

    <div class="section section-header">
        <div class="container">
            <h1>testertje</h1>
        </div>
    </div>

    <div class="article">
        <div class="col-md-4">
            <div class="card context-account">
                <div class="card-heading">
                    <a href="">{!Account.Name}</a>
                </div>
                <ul class="card-detail">
                    <li>{!Account.BillingStreet}</li>
                    <li>{!Account.BillingPostalCode}, {!Account.BillingCity}</li>
                </ul>
            </div>
        </div>
    </div>
</body>
</apex:page>